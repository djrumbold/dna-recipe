{
  "name": "dnadesign/basic-recipe",
  "description": "DNA Basic Recipe metapackage",
  "type": "silverstripe-module",
  "keywords": ["silverstripe", "dna"],
  "require": {
    "php": ">=5.6.0",
    "benmanu/silverstripe-styleguide": "^0.1",
    "dnadesign/silverstripe-dna": "^2.0",
    "jonom/silverstripe-betternavigator": "^4.0",
    "silverstripe/admin": "^1.0",
    "silverstripe/asset-admin": "^1.0",
    "silverstripe/assets": "^1.0",
    "silverstripe/campaign-admin": "^1.0",
    "silverstripe/cms": "^4.0",
    "silverstripe/config": "^1.0",
    "silverstripe/errorpage": "^1.0",
    "silverstripe/framework": "^4.0",
    "silverstripe/graphql": "^1.0",
    "silverstripe/reports": "^4.0",
    "silverstripe/siteconfig": "^4.0",
    "silverstripe/userforms": "^5.0",
    "silverstripe/versioned": "^1.0",
    "adrexia/standardsediting": "^2.0",
    "stevie-mayhew/silverstripe-svg": "^2.1",
    "symbiote/silverstripe-gridfieldextensions": "^3.1",
    "unclecheese/betterbuttons": "dev-master",
    "silverstripe/raygun": "dev-master"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:dnadesign/silverstripe-dna.git"
    }
  ],
  "require-dev": {
    "phpunit/PHPUnit": "^5.7",
    "silverstripe/testsession": "^2.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    },
    "installer-paths": {
      "cap": ["dnadesign/shipistrano"]
    }
  },
  "config": {
    "process-timeout": 600
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
