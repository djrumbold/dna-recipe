{
  "name": "dnadesign/basic-recipe",
  "description": "DNA Basic Recipe metapackage",
  "type": "silverstripe-module",
  "keywords": ["silverstripe", "dna"],
  "require": {
    "php": ">=5.6.0",
    "benmanu/silverstripe-styleguide": "dev-feature/silverstripe4",
    "dnadesign/silverstripe-dna": "dev-feature/silverstripe4",
    "dnadesign/shipistrano": "dev-master",
    "jonom/silverstripe-betternavigator": "dev-master",
    "silverstripe/admin": "1.0.x-dev",
    "silverstripe/asset-admin": "1.0.x-dev",
    "silverstripe/assets": "1.0.x-dev",
    "silverstripe/campaign-admin": "1.0.x-dev",
    "silverstripe/cms": "4.0.x-dev",
    "silverstripe/config": "1.0.x-dev",
    "silverstripe/errorpage": "1.0.x-dev",
    "silverstripe/framework": "4.0.x-dev",
    "silverstripe/graphql": "0.2.x-dev",
    "silverstripe/reports": "4.0.x-dev",
    "silverstripe/siteconfig": "4.0.x-dev",
    "silverstripe/versioned": "1.0.x-dev",
    "silverstripe/standardsediting": "dev-master",
    "stevie-mayhew/silverstripe-svg": "dev-feature/silverstripe4",
    "symbiote/silverstripe-gridfieldextensions": "dev-master",
    "unclecheese/betterbuttons": "dev-master",
    "silverstripe/raygun": "dev-patch-1 as dev-master"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:dnadesign/silverstripe-dna.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/dnadesign/shipistrano.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/adrexia/silverstripe-raygun.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/adrexia/silverstripe-styleguide.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/adrexia/silverstripe-svg.git"
    }
  ],
  "require-dev": {
    "phpunit/PHPUnit": "^5.7",
    "silverstripe/testsession": "dev-master"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    },
    "installer-paths": {
      "cap": ["dnadesign/shipistrano"]
    }
  },
  "config": {
    "process-timeout": 600
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
